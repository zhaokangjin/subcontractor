<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.accenture.subcontractor.job.persistence.FileRegistryMapper">
	<resultMap id="BaseResultMap" type="com.accenture.subcontractor.job.domain.FileRegistry">
		<id column="FILEMD5" jdbcType="CHAR" property="filemd5" />
		<result column="FILE_PATH" jdbcType="VARCHAR" property="filePath" />
		<result column="FILE_SIZE" jdbcType="VARCHAR" property="fileSize" />
		<result column="DELETE_FLAG" jdbcType="CHAR" property="deleteFlag" />
		<result column="CREATOR" jdbcType="CHAR" property="creator" />
		<result column="CREATE_TIME" jdbcType="TIMESTAMP" property="createTime" />
		<collection column="FILEINFO_LIST" property="fileInfoList"
			ofType="com.accenture.subcontractor.job.domain.FileInfo" javaType="java.util.ArrayList"
			select="selectSubTableByForeignKey" />
	</resultMap>
	<sql id="Base_Column_List">
		FILEMD5, FILE_PATH,FILE_SIZE, DELETE_FLAG,
		CREATOR, CREATE_TIME
	</sql>
	<!-- <sql id="Sub_Column_List"> FILE_ID, ORIGINAL_FILE, FILEMD5, ACTUAL_FILE_NAME, 
		DELETE_FLAG, CREATOR, CREATE_TIME </sql> -->
	<select id="selectByPrimaryKey" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from TFT_FILE_REGISTRY
		where FILEMD5 = #{primaryKey,jdbcType=CHAR}
	</select>
	<resultMap type="com.accenture.subcontractor.job.domain.FileInfo" id="subTable">
		<id column="FILE_ID" jdbcType="CHAR" property="fileId" />
		<result column="ORIGINAL_FILE" jdbcType="VARCHAR" property="originalFile" />
		<result column="FILEMD5" jdbcType="CHAR" property="filemd5" />
		<result column="ACTUAL_FILE_NAME" jdbcType="VARCHAR" property="actualFileName" />
		<result column="DELETE_FLAG" jdbcType="CHAR" property="deleteFlag" />
		<result column="CREATOR" jdbcType="CHAR" property="creator" />
		<result column="CREATE_TIME" jdbcType="TIMESTAMP" property="createTime" />
	</resultMap>
	<select id="selectSubTableByForeignKey" resultMap="subTable">
		select
		FILE_ID, ORIGINAL_FILE, FILEMD5, ACTUAL_FILE_NAME, DELETE_FLAG,
		CREATOR, CREATE_TIME from
		TFT_FILE_INFO t
		<!-- where t.FILEMD5 = #{filemd5,jdbcType=CHAR} -->
	</select>
	<insert id="insert" parameterType="com.accenture.subcontractor.job.domain.FileRegistry">
		insert into TFT_FILE_REGISTRY
		(FILEMD5, FILE_PATH,
		FILE_SIZE, DELETE_FLAG, CREATOR,
		CREATE_TIME)
		values (#{filemd5,jdbcType=CHAR},
		#{filePath,jdbcType=VARCHAR}, #{rerCount,jdbcType=INTEGER},
		#{fileSize,jdbcType=VARCHAR}, #{deleteFlag,jdbcType=CHAR},
		#{creator,jdbcType=CHAR},
		#{createTime,jdbcType=TIMESTAMP})
	</insert>
	<insert id="insertSelective" parameterType="com.accenture.subcontractor.job.domain.FileRegistry">
		insert into TFT_FILE_REGISTRY
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="filemd5 != null">
				FILEMD5,
			</if>
			<if test="filePath != null">
				FILE_PATH,
			</if>
			<if test="fileSize != null">
				FILE_SIZE,
			</if>
			<if test="deleteFlag != null">
				DELETE_FLAG,
			</if>
			<if test="creator != null">
				CREATOR,
			</if>
			<if test="createTime != null">
				CREATE_TIME,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="filemd5 != null">
				#{filemd5,jdbcType=CHAR},
			</if>
			<if test="filePath != null">
				#{filePath,jdbcType=VARCHAR},
			</if>
			<if test="rerCount != null">
				#{rerCount,jdbcType=INTEGER},
			</if>
			<if test="fileSize != null">
				#{fileSize,jdbcType=VARCHAR},
			</if>
			<if test="deleteFlag != null">
				#{deleteFlag,jdbcType=CHAR},
			</if>
			<if test="creator != null">
				#{creator,jdbcType=CHAR},
			</if>
			<if test="createTime != null">
				#{createTime,jdbcType=TIMESTAMP},
			</if>
		</trim>
	</insert>
	<insert id="insertBatch" parameterType="java.util.List">
		insert into TFT_FILE_REGISTRY
		(
		<include refid="Base_Column_List" />
		) values
		<foreach collection="list" item="item" index="index"
			separator=",">
			(
			#{item.filemd5,jdbcType=CHAR},
			#{item.filePath,jdbcType=VARCHAR},
			#{item.fileSize,jdbcType=VARCHAR}, #{item.deleteFlag,jdbcType=CHAR},
			#{item.creator,jdbcType=CHAR},
			#{item.createTime,jdbcType=TIMESTAMP}
			)
		</foreach>
	</insert>
	<!-- 物理删除 -->
	<delete id="deleteByPrimaryKey" parameterType="java.lang.String">
		delete from
		TFT_FILE_REGISTRY
		where FILEMD5 = #{primaryKey,jdbcType=CHAR}
	</delete>
	<!-- 物理批量删除 -->
	<delete id="deleteList" parameterType="java.util.List">
		DELETE FROM TFT_FILE_REGISTRY where FILE_ID in
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			#{item.fileId,jdbcType=CHAR}
		</foreach>
	</delete>
	<!-- 软删除 -->
	<update id="deleteByPrimaryKeySoft" parameterType="com.accenture.subcontractor.job.domain.FileRegistry">
		update
		TFT_FILE_REGISTRY SET
		DELETE_FLAG = 'Y'
		where FILEMD5 =
		#{primaryKey,jdbcType=CHAR}
	</update>
	<!-- 软批量删除 -->
	<update id="deleteListSoft" parameterType="com.accenture.subcontractor.job.domain.FileRegistry">
		update TFT_FILE_REGISTRY SET
		DELETE_FLAG = 'Y'
		where FILEMD5 in
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			#{item.filemd5,jdbcType=CHAR}
		</foreach>
	</update>

	<update id="updateByPrimaryKeySelective" parameterType="com.accenture.subcontractor.job.domain.FileRegistry">
		update TFT_FILE_REGISTRY
		<set>
			<if test="filePath != null">
				FILE_PATH = #{filePath,jdbcType=VARCHAR},
			</if>
			<if test="fileSize != null">
				FILE_SIZE = #{fileSize,jdbcType=VARCHAR},
			</if>
			<if test="deleteFlag != null">
				DELETE_FLAG = #{deleteFlag,jdbcType=CHAR},
			</if>
		</set>
		where FILEMD5 = #{filemd5,jdbcType=CHAR}
	</update>
	<update id="updateByPrimaryKey" parameterType="com.accenture.subcontractor.job.domain.FileRegistry">
		update
		TFT_FILE_REGISTRY
		set FILE_PATH = #{filePath,jdbcType=VARCHAR},
		FILE_SIZE =
		#{fileSize,jdbcType=VARCHAR},
		DELETE_FLAG = #{deleteFlag,jdbcType=CHAR}
		where FILEMD5 = #{filemd5,jdbcType=CHAR}
	</update>
	<update id="updateList" parameterType="com.accenture.subcontractor.job.domain.FileRegistry">
		<foreach collection="list" item="item" index="index" open=""
			close="" separator=";">
			<if test="item.filemd5 != null">
				update TFT_FILE_REGISTRY
				<set>
					<if test="item.filePath != null">
						FILE_PATH = #{filePath,jdbcType=VARCHAR},
					</if>
					<if test="item.fileSize != null">
						FILE_SIZE = #{fileSize,jdbcType=VARCHAR},
					</if>
					<if test="item.deleteFlag != null">
						DELETE_FLAG = #{deleteFlag,jdbcType=CHAR},
					</if>
				</set>
				where FILE_ID = #{item.fileId,jdbcType=CHAR}
			</if>
			<!-- FILEMD5, FILE_PATH,FILE_SIZE, DELETE_FLAG, CREATOR, CREATE_TIME -->
			<if test="item.fileId = null">
				insert into TFT_FILE_REGISTRY (
				<include refid="Base_Column_List" />
				) values
				(
				#{item.filemd5,jdbcType=CHAR},
				#{item.filePath,jdbcType=VARCHAR},
				#{item.rerCount,jdbcType=INTEGER},
				#{item.fileSize,jdbcType=VARCHAR}, #{item.deleteFlag,jdbcType=CHAR},
				#{item.creator,jdbcType=CHAR},
				#{item.createTime,jdbcType=TIMESTAMP}
				)
			</if>
		</foreach>
	</update>
	<select id="queryList" parameterType="com.accenture.subcontractor.job.domain.FileRegistry"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from TFT_FILE_REGISTRY
		where
		1=1
		<if test="filemd5!=null">
			AND FILEMD5 = #{filemd5,jdbcType=CHAR}
		</if>
		<if test="filePath != null">
			AND FILE_PATH = #{filePath,jdbcType=VARCHAR},
		</if>
		<if test="fileSize != null">
			AND FILE_SIZE = #{fileSize,jdbcType=VARCHAR},
		</if>
		<if test="deleteFlag != null">
			AND DELETE_FLAG = #{deleteFlag,jdbcType=CHAR},
		</if>
	</select>

</mapper>